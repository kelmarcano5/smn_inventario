<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrol.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="7" cellStep="0" name="${lbl:b_rol}"/>
	
	<excel 
		recordset='query-smnrol.sql' 
		sheetname='${lbl:b_rol}' 
		filename='${lbl:b_rol}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_usuarios_rf_combo' label='${lbl:b_users}' />
		<col id='rol_tipo_combo' label='${lbl:b_type}' />
		<col id='smn_corporaciones_rf_combo' label='${lbl:b_corporations}' />
		<col id='smn_entidades_rf_combo' label='${lbl:b_entities}' />
		<col id='smn_sucursales_rf_combo' label='${lbl:b_subsidiaries}' />
		<col id='smn_areas_servicios_rf_combo' label='${lbl:b_service_areas}' />
		<col id='smn_unidades_servicios_rf_combo' label='${lbl:b_service_units}' />
		<col id='eor_nombre_combo' label='${lbl:b_posicion_estructura}' />
		<col id='rol_estatus_combo' label='${lbl:b_status}' />
		<col id='rol_vigencia' label='${lbl:b_validation_date}' />
		<col id='rol_idioma' label='${lbl:s_profile_lang}' />
		<col id='rol_usuario' label='${lbl:b_users}' />
		<col id='rol_fecha_registro' label='${lbl:b_store_date}' />
		<col id='rol_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
