<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_usuarios_rf' type='integer' required='false' label='${lbl:b_users}' maxlen='10'/>
	<parameter id='rol_tipo' type='varchar' required='false' label='${lbl:b_type}' maxlen='2'/>
	<parameter id='smn_corporaciones_rf' type='integer' required='false' label='${lbl:b_corporations}' maxlen='10'/>
	<parameter id='smn_entidades_rf' type='integer' required='false' label='${lbl:b_entities}' maxlen='10'/>
	<parameter id='smn_sucursales_rf' type='integer' required='false' label='${lbl:b_subsidiaries}' maxlen='10'/>
	<parameter id='smn_areas_servicios_rf' type='integer' required='false' label='${lbl:b_service_areas}' maxlen='10'/>
	<parameter id='smn_unidades_servicios_rf' type='integer' required='false' label='${lbl:b_service_units}' maxlen='10'/>
	<parameter id='rol_posicion_estructura_rf' type='integer' required='false' label='${lbl:b_posicion_estructura}' maxlen='10'/>
	<parameter id='rol_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<parameter id='rol_vigencia' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='rol_tipo'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='rol_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
