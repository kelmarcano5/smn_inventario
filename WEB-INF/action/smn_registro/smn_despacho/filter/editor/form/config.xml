<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_despacho_id}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_almacen_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_zona_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_ruta_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_transporte_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_direccion_rf.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_almacen_rf.sql' tag='smn_almacen_rf_rows'/>
		<print mode='table' recordset='query-smn_zona_rf.sql' tag='smn_zona_rf_rows'/>
		<print mode='table' recordset='query-smn_ruta_id.sql' tag='smn_ruta_id_rows'/>
		<print mode='table' recordset='query-smn_transporte_id.sql' tag='smn_transporte_id_rows'/>
		<print mode='table' recordset='query-smn_direccion_rf.sql' tag='smn_direccion_rf_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
